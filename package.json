{
	"name": "onlyoffice-directus",
	"description": "This extension allows working with office files in Directus using ONLYOFFICE Docs.",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "onlyoffice",
				"source": "src/api/index.ts"
			},
			{
				"type": "module",
				"name": "module",
				"source": "src/modules/settings/index.ts"
			},
			{
				"type": "interface",
				"name": "onlyoffice-file-interface",
				"source": "src/onlyoffice-file-interface/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build --sourcemap && pnpm copy-assets",
		"dev": "directus-extension build -w --no-minify",
		"copy-assets": "copyfiles -u 1 ./src/assets/document-templates/**/*.{docx,xlsx,pptx} dist && copyfiles -u 1 ./src/assets/document-templates/**/*.pdf dist",
		"link": "directus-extension link",
		"add": "directus-extension add",
		"res": "pnpm install --ignore-workspace"
	},
	"devDependencies": {
		"@directus/env": "5.2.0",
		"@directus/extensions-sdk": "16.0.2",
		"@directus/utils": "13.0.10",
		"@types/jsonwebtoken": "9.0.7",
		"@types/node": "^22.18.10",
		"copyfiles": "2.4.1",
		"jsonwebtoken": "9.0.2",
		"typescript": "^5.9.3",
		"vue": "^3.5.22"
	},
	"packageManager": "pnpm@10.18.2",
	"engines": {
		"node": ">=22",
		"pnpm": ">=10 <11"
	}
}
